alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias duh="du -hs .[^.]*"

# proxy
alias pc="proxychains4 -q"

# Git
alias ga="git add"
alias gb="git branch"
alias gba="git branch -av"
alias gd="git diff"
alias gdc="git diff --cached"
alias gfa="git fetch --all"
alias ghf="git log --follow -p --"
alias grh="git reset --soft HEAD^ && git reset --hard HEAD"
alias gss="git status -s"
